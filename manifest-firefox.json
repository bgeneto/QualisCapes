{
  "name": "Qualis Capes",
  "description": "Utiliza o novo critério da CAPES para exibir o fator Qualis Periódico diretamente nas páginas do scopus.com",
  "version": "1.0.1",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "https://www.scopus.com/source*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_title": "Qualis Capes"
  },
  "icons": {
    "16": "img/icon16.png",
    "32": "img/icon32.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  }
}